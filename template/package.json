{
    "name": "loyaltyDemo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "lint": "eslint .",
        "test": "jest",
        "postinstall": "patch-package ",
        "clean-cache": "watchman watch-del-all &&  rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
        "reinstall-node-modules": "rm -rf node_modules && rm yarn.lock  && yarn",
        "reinstall-clean-ios": "cd ios && rm Podfile.lock && rm -rf Pods && rm -rf loyaltyDemo.xcworkspace && cd .. && npx pod-install && yarn ios",
        "reinstall-clean-android": "cd android && ./gradlew clean && cd .. && adb reverse tcp:8081 tcp:8081 && yarn android",
        "clean-reinstall-all": "yarn clean-cache && yarn reinstall-node-modules && yarn reinstall-clean-ios && yarn reinstall-clean-android"
    },
